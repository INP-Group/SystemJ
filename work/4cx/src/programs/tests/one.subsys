# A comment

sysname example
defserver :0
treeplace /

define(`kukareku',
       `knob /fixedranges,fixedstep,groupable \
         set3 "A control"  text
')

grouping main mainGrouping "Main screen" grid - 2 0 0 "" "" "" 2 {
    disp mes1 "Some value" text noparams:1 \
    	r:abc.def w:%reg1 c:"_all_code
		noop; sleep 20;
		sleep 10
#		print_str zzz
		push 3.1415
		putchan chan.with.prefix; ret"
   kukareku
} tip:"This is a main grouping"

wintitle main "An example window (При-мер!)" #z
winname  main example
winclass main Example
winopts  main resizable,nostatusline,compact
