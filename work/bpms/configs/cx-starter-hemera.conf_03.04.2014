VEPP_BPM_CONF   chaninfo=hemera:4!-1 \
		start="exec /mnt/common/rogovsky/bin/v2k_bpm_cfg hemera:4,chan_base:0 -name v2k_bpm_cfg 1>/dev/null 2>/dev/null" \
                app_name=v2k_bpm_cfg comment="configuration utility for BPMs of VEPP-2000"
VEPP_BPM_1_TBT	foreign \
		start="exec /mnt/common/rogovsky/bin/tbt_viewer --ringid=vepp2k --bpmdid=bpm1 --tbtsrvr=hemera:4,chan_base:0,bigc_base:4 -name tbt_bpm1 1>/dev/null 2>/dev/null" \
                app_name=tbt_bpm1  comment="TBT Data Viewer for BPM#1 of VEPP-2000"
VEPP_BPM_2_TBT	foreign \
		start="exec /mnt/common/rogovsky/bin/tbt_viewer --ringid=vepp2k --bpmdid=bpm2 --tbtsrvr=hemera:4,chan_base:0,bigc_base:5 -name tbt_bpm2  1>/dev/null 2>/dev/null" \
                app_name=tbt_bpm2  comment="TBT Data Viewer for BPM#2 of VEPP-2000"
VEPP_BPM_3_TBT	foreign \
		start="exec /mnt/common/rogovsky/bin/tbt_viewer --ringid=vepp2k --bpmdid=bpm3 --tbtsrvr=hemera:4,chan_base:0,bigc_base:6 -name tbt_bpm3  1>/dev/null 2>/dev/null" \
                app_name=tbt_bpm3  comment="TBT Data Viewer for BPM#3 of VEPP-2000"
VEPP_BPM_4_TBT	foreign \
		start="exec /mnt/common/rogovsky/bin/tbt_viewer --ringid=vepp2k --bpmdid=bpm4 --tbtsrvr=hemera:4,chan_base:0,bigc_base:7 -name tbt_bpm4  1>/dev/null 2>/dev/null" \
                app_name=tbt_bpm4  comment="TBT Data Viewer for BPM#4 of VEPP-2000"
