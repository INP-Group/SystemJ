xterm_hemera	foreign \
		start="exec ssh hemera -f xterm -bg BLACK -fg WHITE -fn 9x15 -name myterm 2>&1 >/dev/null" \
		app_name=myterm comment="terminal on hemera"

kill-servers    foreign \
		app_name="-NON-EXISTENT-" comment="выключаем ВСЕ CX серверы на машине HEMERA" \
                start="kill `cat /var/tmp/cxd-*.pid`"
